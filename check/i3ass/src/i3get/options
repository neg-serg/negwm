[--class|-c CLASS]
[--instance|-i INSTANCE]
[--title|-t TITLE]
[--conid|-n CON_ID]
[--id|-d WIN_ID]
[--winid WIN_ID]
[--mark|-m MARK]
[--titleformat|-o TITLE_FORMAT]
[--role ROLE]
[--type TYPE]
[--synk|-y]
[--print|-r OUTPUT]
[--print-format FORMAT_STRING]
[--json TREE]
--timeout SECONDS
--help|-h
--version|-v
--verbose
